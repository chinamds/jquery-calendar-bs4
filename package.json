{
  "name": "arrobefr-jquery-calendar-bs4",
  "version": "1.0.3",
  "description": "A responsive jquery calendar scheduler built with bootstrap 4 and moment.js",
  "main": "dist/js/jquery-calendar.min.js",
  "scripts": {
    "css-minify": "cleancss --level 2 --output dist/css/jquery-calendar.min.css src/css/jquery-calendar.css",
    "js-minify": "uglifyjs --compress --mangle --toplevel --comments /^@/ --output dist/js/jquery-calendar.min.js src/js/jquery-calendar.js",
    "start": "node app",
    "test": "grunt test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ArrobeFr/jquery-calendar-bs4.git"
  },
  "keywords": [
    "jquery-plugin",
    "ecosystem:jquery",
    "moment.js",
    "bootstrap",
    "bootstrap4",
    "bootstrap 4",
    "calendar",
    "scheduler",
    "responsive"
  ],
  "author": "Arrobe Fr <contact@arrobe.fr> (https://www.arrobe.fr)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ArrobeFr/jquery-calendar-bs4/issues"
  },
  "homepage": "https://github.com/ArrobeFr/jquery-calendar-bs4#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free-webfonts": "^1",
    "bootstrap": "^4",
    "jquery": "^3",
    "jquery-touchswipe": "^1.6",
    "moment": "^2.22",
    "popper.js": "^1.14"
  },
  "devDependencies": {
    "clean-css-cli": "^4",
    "uglify-js": "^3",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "bower": "^1.8.8",
    "express": "3.4.x",
    "grunt": "^1.0.1",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-qunit": "^3.1.0",
    "grunt-contrib-uglify": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "phantomjs": "^2.1.7",
    "qunit": "^2.9.1"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE"
  ]
}
